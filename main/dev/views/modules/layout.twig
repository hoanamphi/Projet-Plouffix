<!DOCTYPE html>
<html lang="fr-FR">
    <head>
        <meta charset="utf-8"/>
        <meta name="viewport" content="width=device-width, height=device-height, shrink-to-fit=no"/>
        <title>{% block title %}{% endblock %}</title>


        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" href="../../../public/assets/css/index.css">
        {% endblock %}


        {% block scripts %}
        {% endblock %}
    </head>

    <body>
        {% include "modules/flash.twig" with {"flash": flash} %}
        {% block body %}
        <div class="block">
            <p>Patate</p>
            {% block dataDisplay %}
                <ul>
                    {% for line in data %}
                        <li>{{ line }}</li>
                    {% endfor %}
                </ul>
            {% endblock %}
        </div>
        {% endblock %}
    </body>
</html>
